{
	"entryFile": "src/index.ts",
	"noImplicitAdditionalProperties": "throw-on-extras",
	"controllerPathGlobs": [
		"./src/v2/**/*",
		"src/v2/**/*controller.ts"
	],
	"spec": {
		"name": "Faithful API",
		"basePath": "v2",
		"outputDirectory": "public/v2",
		"specVersion": 3,
		"specMerging": "recursive",
		"securityDefinitions": {
			"bot": {
				"type": "apiKey",
				"name": "bot",
				"in": "header"
			},
			"discord": {
				"type": "apiKey",
				"name": "discord",
				"in": "header"
			},
			"cloudflare": {
				"type": "apiKey",
				"name": "cloudflare",
				"in": "header"
			}
		},
		"paths": {
			"/addons": {
				"post": {
					"consumes": [
						"multipart/form-data"
					],
					"parameters": [
						{
							"in": "formData",
							"name": "headerFile",
							"required": true,
							"type": "file"
						}
					]
				}
			}
		}
	},
	"routes": {
		"basePath": "v2",
		"routesDir": "build/v2",
		"authenticationModule": "src/common/tools/authentication.ts"
	},
	"compilerOptions": {
		"baseUrl": "src",
		"paths": {
			"@common/*": ["common/*"],
			"@v1/*": ["v1/*"],
			"@v2/*": ["v2/*"]
		}
	}
}